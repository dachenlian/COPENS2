INFO "Brown"
NAME "Brown"
ENCODING "UTF-8"
LANGUAGE "English"
PATH "/var/lib/manatee/data/brown"
VERTICAL "/var/lib/manatee/vert/brown.vert"
TAGSETDOC "https://www.ling.upenn.edu/courses/Fall_2003/ling001/penn_treebank_pos.html"
FULLREF "doc.category,doc.n"

ATTRIBUTE word

ATTRIBUTE lemma

ATTRIBUTE tag

ATTRIBUTE   lc {
	LABEL	"word (lowercase)"
	DYNAMIC  utf8lowercase
	DYNLIB   internal
	ARG1     "C"
	FUNTYPE  s
	FROMATTR word
	TYPE     index
	TRANSQUERY	yes
}

ATTRIBUTE   lemma_lc {
	LABEL	"lemma (lowercase)"
	DYNAMIC  utf8lowercase
	DYNLIB   internal
	ARG1     "C"
	FUNTYPE  s
	FROMATTR lemma
	TYPE     index
	TRANSQUERY	yes
}

STRUCTURE doc {
	ATTRIBUTE category
	ATTRIBUTE wordcount
	ATTRIBUTE n
}

STRUCTURE s